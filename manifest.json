{
	"name": "TP's Bitcoin Ticker",
	"version": "1.0",
	"permissions": [
		"activeTab"
	],
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "icon128.png",
		"default_popup": "popup.html",
		"default_title": "TP's Bitcoin Ticker"
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"manifest_version": 2,
	"permissions": [
		"alarms",
		"https://bitso.com/"
	]
}
